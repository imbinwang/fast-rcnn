train_net: "models/wb_CaffeNet/train_linemod_sub.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 2000000
display: 100
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "caffenet_largesub"
#debug_info: true

train_net: "models/wb_CaffeNet/train_linemod_pose_reg.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 20000
display: 1
average_loss: 10
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "caffenet_fast_rcnn"
#debug_info: true
